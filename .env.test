# Environment variables for testing
# This file can be used with act --env-file .env.test

# Testing environment
ENVIRONMENT=test
AZURE_ENVIRONMENT=AzureCloud

# Resource configuration for testing
RESOURCE_GROUP=ShadowPivot-Test
LOCATION=eastus
LOG_LEVEL=debug

# Feature flags for testing
ENABLE_VERBOSE_LOGGING=true
SKIP_CLEANUP=false
DRY_RUN=false

# Testing-specific values
TEST_MODE=true
DEPLOYMENT_NAME=test-deployment-$(date +%s)
